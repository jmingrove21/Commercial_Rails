<div style="padding-top:70px">
  <%= form_for @item do |f| %>
    <%=f.label :title%>
    <%=f.text_field :title %>
    <br>
    <br>

    <%=f.label :body%>
    <%=f.text_area :body%>
    <br>
    <br>
    <%=f.label :price%>
    <%=f.text_field :price%>
    <br>
    <br>
    <%= f.fields_for :images do |ff|%>
      <%= ff.label :image%>
      <%= ff.file_field :image, class: "form-control"%>
    <%end%>
    <br>
    <%= f.hidden_field :user_id, value: current_user&.id%>
    <%=f.submit "제출"%>
  <%end%>
</div>